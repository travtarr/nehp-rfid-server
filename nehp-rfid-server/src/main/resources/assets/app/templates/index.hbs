<div class="notifications">
	<div class="title">
		<span>Notifications</span>
	</div>
	<div class="content">
		<ul>
		{{#each}}
			<li class="header">Date:{{date}} &nbsp By:{{created_by}}</li>
			<li class="title">{{title}}</li>
			<li class="message">{{message}}</li>
		{{/each}}
		</ul>
	</div>
</div>