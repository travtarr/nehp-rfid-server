	<div id="wrap">
		<section id="rfid">
		
			<!--  Navigation bar -->
			<div class="navbar navbar-inverse navbar-fixed-top">
				<div class="navbar-inner" style="padding-top: 10px;">
        <div class="container-fluid">
					{{#link-to 'index' classNames="pull-left pad-lg link title-link"}}NEHP Worldwide Tracker{{/link-to}}
					<div>
						<ul class="nav navbar-nav">
							{{#if isAuthenticated}} 
								{{#link-to 'summary' tagName='li' classNames='pad-sm link'}}Summary{{/link-to}}
								{{#link-to 'items' tagName='li' classNames='pad-sm link'}}Status{{/link-to}}
							{{/if}}
						</ul>
						<ul class="nav pull-right">
							{{#if isAuthenticated}} 
								{{#link-to 'user' currentUser.id tagName='li' classNames='link'}}{{currentUser.username}}{{/link-to}} 
								{{#if isAdmin}}
									{{#link-to 'admin' tagName='li' classNames='link'}}Admin{{/link-to}}
								{{/if}}
								<li {{action 'logout'}} class="btn-logout">Logout</li>
							{{else}} 
								{{#link-to 'sessions' tagName='li' classNames='link'}}Login{{/link-to}} 
							{{/if}}
						</ul>
          </div>
        </div>
				</div>
			</div>
			<!-- /Navigation Bar -->

			<!-- Content -->
			<div class="container-fluid">
				<section id="content">
					<!--  Page Contents from Ember -->
					{{outlet}}
				</section>
			</div>
			<!-- /Content -->
		</section>
		{{#unless notification.closed}}
			<div class="notify-container">
				{{notify-alert notification=notification}}
			</div>
		{{/unless}}
	</div>
	<footer>
		<div class="container" id="footer">
			<span>NEHP Worldwide, LLC. Copyright 2014.</span>
		</div>
	</div>